{
  "name": "reading-notes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export && touch out/.nojekyll",
    "lint": "next lint --max-warnings=0",
    "type-check": "tsc",
    "deploy": "gh-pages -d out -t true",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "16.0.10",
    "nextra": "^2.4.2",
    "nextra-theme-docs": "^2.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "24.10.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.9",
    "eslint-config-prettier": "^10.1.8",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "src/**/*.{ts,json,md}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  }
}
